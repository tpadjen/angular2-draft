{
  "name": "ang",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "clean-dev": "rm -rf build/dev",
    "sync-dev": "./node_modules/sync-files/bin/sync-files.js src build/dev",
    "start": "live-server --open=build/dev --entry-file=index.html --ignore=test,src,node_modules",
    "unit": "./node_modules/karma/bin/karma start",
    "e2e": "protractor",
    "watch-ts": "onchange '{build/dev,test}/**/*.ts' -- npm run ts-changed",
    "watch-src": "onchange 'src/**/*.*' -- npm run src-changed",
    "ts-changed": "npm run tsc",
    "src-changed": "npm run sync-dev",
    "watch": "npm run watch-ts & npm run watch-src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "angular2": "2.0.0-alpha.44",
    "ng2-bootstrap": "^0.44.3",
    "systemjs": "0.19.2"
  },
  "devDependencies": {
    "es6-module-loader": "^0.17.8",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.14",
    "karma-chrome-launcher": "^0.2.1",
    "karma-jasmine": "^0.3.6",
    "karma-verbose-reporter": "0.0.3",
    "live-server": "^0.8.2",
    "onchange": "^2.0.0",
    "sync-files": "^1.0.3",
    "typescript": "^1.6.2"
  }
}
